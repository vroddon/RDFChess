        </rdfs:subPropertyOf>
        <rdfs:label>hasHalfMove</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasLastHalfMove -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasLastHalfMove">
