        </rdfs:subClassOf>
        <rdfs:label>ChessOpening</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#ChessTournament -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#ChessTournament">
        <rdfs:subClassOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>ChessCompetitionRound</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#Event -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#Event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#atPlace"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#atPlace"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#Place"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#subEventOf"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#Event"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#atTime"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#atTime"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#HalfMove -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#HalfMove">
        <rdfs:subClassOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#Event"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#nextHalfMove"/>
                        <owl:hasSelf rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasSelf>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#providesAgentRole"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ActingPlayerRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#nextHalfMove"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#HalfMove"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#subEventOf"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ChessGame"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasHalfMove"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ChessGame"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#nextHalfMove"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#HalfMove"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasSANRecord"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>HalfMove</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#Place -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#Place">
