    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#ActingPlayerRole -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#ActingPlayerRole">
        <rdfs:subClassOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#AgentRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#providesAgentRole"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#HalfMove"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>ActingPlayerRole</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#Agent -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#Agent">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Agent</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#AgentRole -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#AgentRole">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#performedBy"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#performedBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>AgentRole</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#AuthorRole -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#AuthorRole">
        <rdfs:subClassOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#AgentRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#providesAgentRole"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ChessGameReport"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>AuthorRole</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#BlackPlayerRole -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#BlackPlayerRole">
        <rdfs:subClassOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#AgentRole"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#providesAgentRole"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ChessGame"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>BlackPlayerRole</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#ChessGame -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#ChessGame">
        <rdfs:subClassOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#Event"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#providesAgentRole"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#BlackPlayerRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#providesAgentRole"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#WhitePlayerRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasHalfMove"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#HalfMove"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasOpening"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ChessOpening"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasReport"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ChessGameReport"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasResult"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ChessGameResult"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#subEventOf"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ChessTournament"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasFirstHalfMove"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#HalfMove"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasLastHalfMove"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#HalfMove"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>ChessGame</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#ChessGameReport -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#ChessGameReport">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasPGNFile"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>ChessGameReport</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#ChessGameResult -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#ChessGameResult">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#encodedAsSAN"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>ChessGameResult</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#ChessOpening -->

    <owl:Class rdf:about="https://w3id.org/rdfchess/ontology/chessgame#ChessOpening">
