        <rdfs:label>encodedAsSAN</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasECOCode -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasECOCode">
        <rdfs:comment>ECO = Encyclopedia of Chess Opening</rdfs:comment>
        <rdfs:label>hasECOCode</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasName -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasName">
        <rdfs:label>hasName</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasPGNFile -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasPGNFile">
