    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#atPlace">
        <rdfs:label>atPlace</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasFirstHalfMove -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasFirstHalfMove">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#hasHalfMove"/>
        <rdfs:label>hasFirstHalfMove</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasHalfMove -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasHalfMove">
        <rdfs:subPropertyOf>
            <rdf:Description>
