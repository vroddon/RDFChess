    <owl:DatatypeProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#atTime">
        <rdfs:label>atTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#encodedAsSAN -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#encodedAsSAN">
