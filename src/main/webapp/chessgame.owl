        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#providesAgentRole"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/rdfchess/ontology/chessgame#ChessGame"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>WhitePlayerRole</rdfs:label>
    </owl:Class>
    


