    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasOpening -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasOpening">
        <rdfs:label>hasOpening</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasReport -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasReport">
        <rdfs:label>hasReport</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasResult -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasResult">
        <rdfs:label>hasResult</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#nextHalfMove -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#nextHalfMove">
        <rdfs:label>nextHalfMove</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#performedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#performedBy">
        <rdfs:label>performedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#providesAgentRole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#providesAgentRole">
