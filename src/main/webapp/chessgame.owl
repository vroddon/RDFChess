    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/rdfchess/ontology/chessgame#hasSANRecord -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/rdfchess/ontology/chessgame#hasSANRecord">
